<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hellow">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".Util.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.FeedbackActivity"></activity>
        <activity android:name=".Activities.ReferEarnActivity" />
        <activity android:name=".Activities.MyProfileActivity" />
        <activity android:name=".Activities.PaymentActivity" />
        <activity android:name=".Activities.ContactUsActivity" />
        <activity android:name=".Activities.PrivacyActivity" />
        <activity android:name=".Activities.AccountSettingsActivity" />
        <activity android:name=".Activities.OptionsActivity" />
        <activity android:name=".Activities.ChatActivity" />
        <activity android:name=".Activities.CreateAccountActivity" />
        <activity android:name=".Activities.GetOtpActivity" />
        <activity android:name=".Activities.OpeningActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>